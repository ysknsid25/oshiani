{
    "name": "functions",
    "description": "Cloud Functions for Firebase",
    "scripts": {
        "serve": "firebase use oshiani-test && firebase emulators:start --only functions",
        "emulate": "firebase use oshiani-test && firebase emulators:start",
        "shell": "firebase use oshiani-test && firebase functions:shell",
        "start": "firebase use oshiani-test && npm run shell",
        "deploy": "firebase use oshiani-test && firebase deploy --only functions",
        "logs": "firebase use oshiani-test && firebase functions:log",
        "firefunctions": "firebase use oshiani && firebase deploy --only functions"
    },
    "engines": {
        "node": "10"
    },
    "main": "index.js",
    "dependencies": {
        "cors": "^2.8.5",
        "firebase-admin": "^8.10.0",
        "firebase-functions": "^3.6.1",
        "nodemailer": "^6.7.2"
    },
    "devDependencies": {
        "firebase-functions-test": "^0.2.0",
        "jsdom": "^19.0.0",
        "node-fetch": "^2.6.2"
    },
    "private": true
}
